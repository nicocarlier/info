{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "next build && touch out/.nojekyll && gh-pages -d out --dotfiles",
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "^5.5.0",
    "react-vertical-timeline-component": "^3.5.3",
    "recharts": "^2.5.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "typescript": "5.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
